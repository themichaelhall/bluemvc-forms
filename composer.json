{
  "name": "bluemvc/bluemvc-forms",
  "description": "Easy and secure form processing for the BlueMvc PHP framework",
  "homepage": "https://bluemvc.com/",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "blue",
    "mvc",
    "framework",
    "php",
    "form",
    "forms"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "datatypes/datatypes": "^3.0",
    "bluemvc/bluemvc-core": "^3.0"
  },
  "require-dev": {
    "bluemvc/bluemvc-fakes": "^3.0",
    "phpunit/phpunit": "^9.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "BlueMvc\\Forms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueMvc\\Forms\\Tests\\": "tests"
    }
  }
}
